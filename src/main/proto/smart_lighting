  rpc TurnOnLight (LightRequest) returns (LightResponse);
  rpc TurnOffLight (LightRequest) returns (LightResponse);
}

// メッセージ定義
message LightRequest {
  string room = 1;
}

message LightResponse {
  string status = 1;
}
